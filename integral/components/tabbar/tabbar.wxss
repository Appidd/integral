.tab-bar {
    background: #fff;
    border-top: 1rpx solid #f7f5f5;
    border-top-left-radius: 35rpx;
    border-top-right-radius: 35rpx;
    bottom: 0;
    height: 86px;
    left: 0;
    position: fixed;
    width: 100%;
    z-index: 999;
}

.tab,.tab-bar {
    align-items: center;
    display: flex;
    justify-content: center;
}

.tab {
    color: #acaaaa;
    flex: 1;
    flex-direction: column;
}

.tab-active {
    color: var(--color-theme);
}

.icon {
    font-size: 40rpx;
    margin-bottom: 15rpx;
}

.icon,.tab-text {
    text-align: center;
    width: 100%;
}

.tab-text {
    font-size: 25rpx;
}