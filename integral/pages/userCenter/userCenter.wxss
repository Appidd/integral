    position: absolute;
    top: 0;
    width: 100%;
    z-index: 200;
}

.down_box_content {
    align-items: center;
    color: #fff;
    display: flex;
    font-size: 25rpx;
    height: 35rpx;
    justify-content: center;
    line-height: 35rpx;
    margin-bottom: 20rpx;
    width: 100%;
    z-index: 300;
}

.down_box_text {
    margin: 0 22rpx 0 9rpx;
}

.modelConfigs {
    background-color: #fff;
    border-radius: 35rpx;
