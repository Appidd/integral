    height: 315rpx;
    justify-content: space-evenly;
    margin-top: 19.5rpx;
    width: 87%;
}

.modelConfigs,.modelConfigs_item {
    display: flex;
}

.modelConfigs_item {
    align-items: center;
    flex-direction: column;
    height: 45%;
    justify-content: center;
    width: 100rpx;
}

.modelConfigs_item image {
    height: 50rpx;
    margin-top: 46rpx;
    width: 51rpx;
}

.modelConfigs_item_text {
    color: #6e7189;
    font-size: 21rpx;
    margin-top: 20rpx;
}

.kvModel1 {
    border-radius: 35rpx;
    height: 341rpx;
    margin-bottom: 50rpx;
    margin-top: 25rpx;
    overflow: hidden;
    width: 87%;
}

.kv_img,.kvModel1_item {
    height: 100%;
    width: 100%;
}

.kvModel1_page {
    align-items: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.lottery_title {
    color: #0d113a;
    font-size: 33rpx;
    font-weight: 700;
    height: 34rpx;
    line-height: 34rpx;
    margin-top: 50rpx;
    padding-left: 17rpx;
    position: relative;
    width: 80%;
}

.lottery_title::before {
    background-color: #afb0be;
    border-radius: 3rpx;
    content: " ";
    height: 34rpx;
    left: -6rpx;
    position: absolute;
    top: 0;
    width: 6rpx;
}

.lottery_content {
    align-items: center;
    display: flex;
    justify-content: space-between;
    margin-top: 39rpx;
    width: 80%;
}

.product_img {
    height: 145rpx;
    width: 145rpx;
}

.lottery_message {
    display: flex;
    flex-direction: column;
    height: 145rpx;
    justify-content: space-between;
    width: calc(100% - 171rpx);
}

.product_name {
    color: #0d113a;
    font-size: 33rpx;
}

.product_price {
    color: #6e7189;
    font-size: 25rpx;
}

.product_bottom {
    align-items: center;
    display: flex;
    justify-content: space-between;
    width: 100%;
}

.product_btn,.product_score {
    color: #df3d4b;
    font-size: 25rpx;
}

.product_btn {
    border: 1rpx solid #df3d4b;
    border-radius: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    width: 153rpx;
}

.hot_goods {
    align-items: center;
    display: flex;
    flex-direction: column;
    width: 87%;
}

.no_new_good {
    margin-bottom: 200rpx;
}

