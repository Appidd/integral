    align-items: center;
    display: flex;
    justify-content: space-between;
    width: 95%;
}

.hot_goods_title {
    color: #313457;
    font-size: 33rpx;
    font-weight: 700;
    height: 34rpx;
    line-height: 34rpx;
    padding-left: 17rpx;
    position: relative;
}

.hot_goods_title::before {
    background-color: #afb0be;
    border-radius: 3rpx;
    content: " ";
    height: 34rpx;
    left: -6rpx;
    position: absolute;
    top: 0;
    width: 6rpx;
}

.into_hot_page {
    align-items: center;
    color: #6e7189;
    display: flex;
    font-size: 25rpx;
}

.more_icon {
    font-size: 22rpx;
}

.hot_goods_bottom {
