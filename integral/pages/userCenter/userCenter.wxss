    justify-content: space-between;
    margin-top: 42rpx;
    width: 100%;
}

.hot_goods_bottom,.hot_goods_item {
    align-items: center;
    display: flex;
}

.hot_goods_item {
    background-color: #fff;
    border-radius: 10rpx;
    flex-direction: column;
    overflow: hidden;
    width: 310rpx;
}

.hot_good_img {
    height: 281rpx;
    margin-top: 5rpx;
