.main_background {
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: -1;
}

.user_center {
    align-items: center;
    display: flex;
    flex-direction: column;
    width: 100%;
}

.big_head_img {
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 1;
}

.logo {
    height: 40rpx;
    overflow: hidden;
    position: absolute;
    top: 50rpx;
    width: 651rpx;
    z-index: 2;
}

.logo image {
    width: 128rpx;
    z-index: 2;
}

.allocation_box {
