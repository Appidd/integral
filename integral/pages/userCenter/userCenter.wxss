    z-index: 200;
}

.up_box_content {
    align-items: center;
    display: flex;
    height: 30rpx;
    justify-content: space-between;
    margin-top: 17rpx;
    width: 85%;
    z-index: 300;
}

.up_box_text {
    color: #fff;
    font-size: 25rpx;
}

.qr_code {
    height: 60rpx;
    justify-content: flex-end;
    padding-top: 20rpx;
    width: 90%;
}

.name_level,.qr_code {
    align-items: center;
    display: flex;
}

.name_level {
    justify-content: center;
    margin-top: 25rpx;
    width: 100%;
}

.need_login {
    background: #0d113a;
    border-radius: 35rpx;
    color: #fff;
    font-size: 18rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    width: 120rpx;
}

.nick_name {
    color: #0d113a;
    font-size: 33rpx;
    margin-right: 13rpx;
    text-align: right;
}

.level {
    align-items: center;
    border-radius: 15rpx;
    color: #fff;
    display: flex;
    font-size: 17rpx;
    height: 30rpx;
    justify-content: center;
    letter-spacing: 5rpx;
    line-height: 30rpx;
    margin-left: 20rpx;
    position: relative;
    width: 117rpx;
}

.level_img {
    border: 1rpx solid #fff;
    border-radius: 50%;
    height: 37rpx;
    left: 0;
    position: absolute;
    transform: translate(-50%,0);
    width: 37rpx;
}

.level_text {
    height: 100%;
    margin-left: 20rpx;
}

.scroes {
    height: 105rpx;
    margin-top: 35rpx;
    width: 90%;
}

.scroes,.scroes_item {
    align-items: center;
    display: flex;
    justify-content: center;
}

.scroes_item {
    flex: 1;
    flex-direction: column;
}

.prices {
    border-right: 2rpx solid #afb0be;
}

.scroes_item_number {
    color: #0d113a;
    font-size: 58rpx;
}

.pastScores {
    color: #df3d4b;
}

.scroes_item_text {
    color: #6e7189;
    font-size: 25rpx;
    margin-top: 13rpx;
