    text-overflow: ellipsis;
    white-space: nowrap;
    width: 281rpx;
}

.hot_good_price {
    color: #6e7189;
    font-size: 17rpx;
    margin-bottom: 27rpx;
    margin-top: 11rpx;
    width: 281rpx;
}

.new_goods {
    align-items: center;
    background-color: #fff;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
